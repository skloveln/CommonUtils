<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.zpc.mvc.user.dao.UserFeedDao" >

  <resultMap id="UserApp" type="cn.zpc.mvc.user.entity.UserFeed">
    <id property="userId" column="user_id"></id>
    <result property="phone" column="phone"></result>
    <result property="content" column="content"></result>
    <result property="create_time" column="createTime"></result>
    <result property="state" column="state"></result>
  </resultMap>

  <insert id="saveFeedContent" parameterType="UserApp" keyProperty="id">
    INSERT INTO
      lb_user_feedback
    (
      user_id,
      phone,
      content,
      create_time,
      state
    ) VALUES (
      #{userId},
      #{phone},
      #{content},
      #{createTime},
      #{state}
    )
  </insert>


</mapper>