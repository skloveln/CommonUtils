<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.zpc.mvc.user.dao.UserImDao" >

  <select id="getToken" resultType="String">
      SELECT
        token
      FROM
        lb_user_im_token
      WHERE
        user_id = #{userId}
  </select>

    <insert id="insert">
        INSERT
        INTO
        lb_user_im_token(
          user_id,
          token
        )VALUES (
          #{userId},
          #{token}
        )
    </insert>
</mapper>